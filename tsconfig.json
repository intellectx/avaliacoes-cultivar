{
  "compilerOptions": {
    "baseUrl": "./resources/js",
    "paths": {
      "@/*": [
        "resources/js/*"
      ]
    },
    "outDir": "public/js",
    "module": "esnext",
    "target": "es5",
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDirs": [
      "resources/js"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "jsx": "preserve"
  },
  "include": [
    "resources/js"
  ],
  "exclude": []
}
